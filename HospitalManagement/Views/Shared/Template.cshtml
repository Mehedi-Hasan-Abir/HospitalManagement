<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <link rel="shortcut icon" type="image/x-icon" href="@Url.Content("~/Content/Template/assets/")img/favicon.ico">
    <title>Hospital Managment </title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Template/assets/")css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Template/assets/")css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Template/assets/")css/style.css">
    <!--[if lt IE 9]>
        <script src="assets/js/html5shiv.min.js"></script>
        <script src="assets/js/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <div class="main-wrapper  account-wrapper">
        <div class="account-page">
            <div class="account-center">
                <div class="account-box">
                    <form action="http://dreamguys.co.in/preclinic/template/index.html" class="form-signin">
                        <div class="account-logo">
                            <a href="index-2.html"><img src="@Url.Content("~/Content/Template/assets/")img/logo-dark.png" alt=""></a>
                        </div>
                        <div class="form-group">
                            <label>Name</label>
                            <input type="text" class="form-control" name="name">
                        </div>
                        <div class="form-group">
                            <label>Gender</label>
                            <input type="text" class="form-control" name="gender">
                        </div>
                        <div class="form-group">
                            <label>Address</label>
                            <input type="text" class="form-control" name="address">
                        </div>
                        <div class="form-group">
                            <label>Blood Group</label>
                            <input type="text" class="form-control" name="bloodGroup">
                        </div>
                        <div class="form-group">
                            <label>Contact Number</label>
                            <input type="text" class="form-control" name="contactNumber">
                        </div>
                        <div class="form-group">
                            <label>Username</label>
                            <input type="text" class="form-control" name="username">
                        </div>
                        <div class="form-group">
                            <label>Password</label>
                            <input type="password" class="form-control" name="password">
                        </div>
                        <div class="form-group text-center">
                            <button class="btn btn-primary account-btn" type="submit">Signup</button>
                        </div>
                        <div class="text-center login-link">
                            Already have an account? <a href="login.html">Login</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script src="@Url.Content("~/Content/Template/assets/")js/jquery-3.2.1.min.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/popper.min.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/bootstrap.min.js"></script>
    <script src="@Url.Content("~/Content/Template/assets/")js/app.js"></script>

    <div>
        @RenderBody()
    </div>
</body>
</html>
